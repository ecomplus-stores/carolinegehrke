<% 

var apx_content = _.cms('apx_looks') || {}

if (Array.isArray(apx_content.list) && apx_content.list.length) {

  const title = apx_content.title  

%> 
    <div class="apx_look">
    <% if (apx_content.container == "Completa"){ %>
            <div class="container-fluid">            
    <% }else{ %>
            <div class="container">
    <% } %>    
            <%- await include(`@/sections/apx_title`, { _,   title}) %>
            <% for (let i = 0; i < apx_content.list.length; i++) { %>
                <div class="col-12 col-md">
                    <div class="item" data-look-id="<%= apx_content.list[i] %>">     
                      <div class="row"></div>                   
                    </div>  
                </div>
            <% } %>                      
        </div>
    </div>
<% } %> 